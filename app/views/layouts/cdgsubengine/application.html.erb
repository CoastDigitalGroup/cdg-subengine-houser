<!DOCTYPE html>
<html>
	<head>
		<title>CoastDigitalGroup Subengine Warden Edition</title>
		<%= stylesheet_link_tag    "subengine/application", media: "all" %>
		<%= javascript_include_tag "subengine/application" %>
		<%= csrf_meta_tags %>
	</head>
	<body>
		<% flash.each do |k,v| %>
			<div class='flash <%= k %>'><%= v %></div>
		<% end %>
		<%= render "subengine/shared/login" %>
		<%= yield %>
	</body>
</html>